@page
@using  Hospitalizacion.App.Dominio;
@model Hospimascotas.App.Frontend.Pages.Auxiliares.EliminarAuxModel
@{
}
<h1>Eliminar Aux</h1>


<form method="post">
    
    <div class="form-group">
     <label asp-for="auxiliar.Id"></label>
     <input asp-for="auxiliar.Id" class="form-control" readonly>

</div>


    <div class="form-group">
     <label asp-for="auxiliar.Nombre"></label>
     <input asp-for="auxiliar.Nombre" class="form-control" required="required">

</div>
<div class="form-group">
     <label asp-for="auxiliar.Apellido"></label>
     <input asp-for="auxiliar.Apellido" class="form-control" required="required">

</div>
<div class="form-group">
     <label asp-for="auxiliar.No_Certificado"></label>
     <input asp-for="auxiliar.No_Certificado" class="form-control" required="required">
</div>
<p>Esta Seguro que quiere Eliminar este Registro</p>
<button type="submit" class="btn btn-secondary">Eliminar<button>  <a class="btn btn-secondary"  asp-page="./ListaAuxiliares">Volver a Auxiliares</a>
  
</form>